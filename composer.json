{
	"name": "rcsv/CakeFC",
	"description": "FullCalendar Implementation using CakePHP framework",
	"type": "library",
	"keywords": ["calendar"],
	"license": "MIT",
	"author": [
		{
			"name": "Rcsv",
			"homepage": "https://github.com/rcsv/CakeFC"
		}
	],
	"config": {
		"vendor-dir": "Vendor/"
	},
	"extra": {
		"installer-paths": {
			"app/Vendor/Facebook": ["facebook/php-sdk-v4"]
		}
	},
	
	"require": {
		"ext-mcrypt": "*",
		"composer/installers": "*",
		"facebook/php-sdk-v4" : "4.0.*"
	},
	"require-dev": {
		"cakephp/debug_kit": "2.2.*",
		"phpunit/phpunit": "3.7.*"
	},
	"bin": [
		"lib/Cake/Console/cake"
	],
	"package": {
		"name": "jquery/jquery",
		"version": "2.1.1",
		"dist": {
			"url": "http://code.jquery.com/jquery-2.1.1.min.js",
			"type": "file"
		}
	}	
}

