{
	"name": "rcsv/CakeFC",
	"description": "FullCalendar Implementation using CakePHP framework",
	"type": "library",
	"keywords": ["calendar"],
	"license": "MIT",
	"author": [
		{
			"name": "Rcsv",
			"homepage": "https://github.com/rcsv/CakeFC"
		}
	],
	"config": {
		"vendor-dir": "Vendor/"
	},
	"extra": {
		"installer-paths": {
			"app/Plugin/DebugKit": ["cakephp/debug_kit"],
			"app/Plugin/Localized": ["cakephp/localized"],
			"app/Plugin/BoostCake": ["slywalker/boost_cake"]
		}
	},
	
	"require": {
		"php": ">=5.5.12",
		"josegonzalez/cakephp-upload": "1.0.0",
		"twbs/bootstrap": ">=3.1.1",
		"slywalker/boost_cake": "*",
		"ext-mcrypt": "*",
		"composer/installers": "*"
	},
	"require-dev": {
		"cakephp/cakephp": ">=2.4.9",
		"cakephp/debug_kit": "2.2.*",
		"cakephp/localized": "2.1.*",
		"phpunit/phpunit": "3.7.*"
	},
	"bin": [
		"lib/Cake/Console/cake"
	]
}

